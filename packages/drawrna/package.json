{
  "name": "drawrnajs",
  "description": "Visualizes RNA secondary structures.",
  "version": "1.2.4",
  "homepage": "https://github.com/bene200/drawrnajs",
  "author": {
    "name": "bene200",
    "email": "berau90@web.de"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bene200/drawrnajs.git"
  },
  "bugs": {
    "url": "https://github.com/bene200/drawrnajs/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/bene200/drawrnajs/blob/master/LICENSE"
    }
  ],
  "main": "src/drawrnajs.js",
  "scripts": {
    "test": "echo 'Error: no test specified' && exit 1",
    "build-js": "mkdirp dist && npx browserify src/entry.js -o dist/index.js",
    "build-css": "npx concat-cli -f style/bootstrap.css style/spectrum.css style/style.css -o dist/index.css",
    "build": "npm run build-js && npm run build-css",
    "build-library": "mkdirp build && browserify -r ./:drawrnajs -o build/drawrnajs.js",
    "build-browser": "npm run build",
    "prepublish": "npm run build",
    "watch": "watchify -r ./:drawrnajs -v -o build/drawrnajs.js",
    "sniper": "sniper ."
  },
  "dependencies": {
    "backbone": "^1.2.2",
    "bootstrap": "^3.3.5",
    "cytoscape": "2.4.6",
    "cytoscape-edgehandles": "2.0.0",
    "jquery": "^2.1.3",
    "tooltip": "^1.6.0",
    "underscore": "^1.3.8"
  },
  "devDependencies": {
    "axios": "^1.9.0",
    "browserify": "6.x",
    "concat-cli": "^4.0.0",
    "exposify": "^0.5.0",
    "mkdirp": "^0.5.0",
    "sniper": "^0.2.16",
    "watchify": "^1.0.6"
  },
  "sniper": {
    "js": [
      "/build/drawrnajs.js"
    ],
    "css": [
      "/style/bootstrap.css",
      "/style/spectrum.css",
      "/style/style.css"
    ],
    "snippets": [
      "examples"
    ],
    "first": "simple"
  },
  "keywords": [
    "rna",
    "secondary structure",
    "biojs",
    "galaxy-vis"
  ],
  "galaxy": {
    "datatypes": [
      "sequence.DotBracket"
    ]
  }
}
