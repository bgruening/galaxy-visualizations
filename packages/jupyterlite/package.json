{
    "name": "jl-galaxy",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build": "webpack && npm run build:jupyter && node install-extension.js",
        "build:jupyter": "jupyter lite build --output-dir static/dist/_output",
        "serve": "jupyter lite serve --output-dir static/dist/_output",
        "prettier": "prettier --write 'package.json' 'install-extension.js' 'src/*.js'"
    },
    "devDependencies": {
        "prettier": "^3.5.3",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "ts-loader": "^9.5.0",
        "typescript": "^5.4.2"
    }
}
